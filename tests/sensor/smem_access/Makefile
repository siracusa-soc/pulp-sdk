APP_CFLAGS += -DADC_D2_LENGTH=$(S_A2L)
APP_CFLAGS += -DROW_D0_LENGTH=$(S_R0L)
APP_CFLAGS += -DROW_D1_LENGTH=$(S_R1L)
APP_CFLAGS += -DROW_D2_LENGTH=$(S_R2L)
APP_CFLAGS += -DFRAME_D0_LENGTH=$(S_F0L) 
APP_CFLAGS += -DFRAME_D1_LENGTH=$(S_F1L)
APP_CFLAGS += -DFRAME_D2_LENGTH=$(S_F2L)
APP_CFLAGS += -DADC_D0_STRIDE=$(S_A0S)
APP_CFLAGS += -DADC_D1_STRIDE=$(S_A1S)
APP_CFLAGS += -DADC_D2_STRIDE=$(S_A2S)
APP_CFLAGS += -DROW_D0_STRIDE=$(S_R0S)
APP_CFLAGS += -DROW_D1_STRIDE=$(S_R1S)
APP_CFLAGS += -DROW_D2_STRIDE=$(S_R2S)
APP_CFLAGS += -DFRAME_D0_STRIDE=$(S_F0S)
APP_CFLAGS += -DFRAME_D1_STRIDE=$(S_F1S)
APP_CFLAGS += -DFRAME_D2_STRIDE=$(S_F2S)


